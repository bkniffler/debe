{
  "name": "debe-workspace",
  "version": "1.0.7",
  "private": true,
  "license": "MIT",
  "keywords": [
    "sqlite",
    "database",
    "electron",
    "react-native",
    "react"
  ],
  "workspaces": [
    "./src/*"
  ],
  "author": "Benjamin Kniffler <bkniffler@me.com>",
  "bugs": {
    "url": "https://github.com/bkniffler/debe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bkniffler/debe.git"
  },
  "scripts": {
    "up": "yarn upgrade-interactive --latest",
    "docs": "docsify serve docs",
    "clean": "rimraf lib && rimraf node_modules/.cache",
    "dev": "run-p \"ts:* -- --watch\"",
    "build": "run-s build:*",
    "build:ts": "tsc --project tsconfig.prod.json",
    "build:rollup": "rollup -c",
    "pub": "yarn test && yarn build && yarn pub:npm",
    "pub:npm": "foreach --no-C -g \"lib/*\" -x \"npm publish #{path} --registry=https://registry.npmjs.org/\"",
    "test": "jest --verbose=false",
    "test2": "yarn ts:lib && PG_CONNECTIONSTRING=postgresql://postgres@localhost:5432/test2 jest --verbose=false",
    "coverage": "codecov"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "@types/react": "^16.8.10",
    "@types/react-test-renderer": "^16.0.3",
    "codecov": "^3.1.0",
    "docsify-cli": "^4.3.0",
    "foreach-cli": "^1.8.1",
    "fs-extra": "^7.0.1",
    "jest": "24.5.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^1.7.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "ts-jest": "^24.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1"
  },
  "resolutions": {
    "@types/react": "^16.8.10"
  }
}
